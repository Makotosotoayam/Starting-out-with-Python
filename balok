print("="*25)
print("balok")
print("="*25)

p = int(input("masukan nilai panjang\t: "))
l = int(input("masukan nilai lebar\t: "))
t = int(input("masukan nilai tinggi\t: "))

luas = (p * l) + (p * t) + (l * t)
volume = p * l * t
keliling = 4 * (p + l + t)

print(f"LUAS\t\t: {luas(p,l,t)} cm2")
print(f"VULUME\t: {volume(p,l,t)} cm2")
print(f"KELILING\t : {keliling(p,l,t)} cm2")
